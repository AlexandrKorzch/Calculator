<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_dev"
        style="@style/btn_action_land_style"
        android:text="@string/dev" />

    <Button
        android:id="@+id/btn_mults"
        style="@style/btn_action_land_style"
        android:layout_below="@id/btn_dev"
        android:text="@string/mult" />

    <Button
        android:id="@+id/btn_minus"
        style="@style/btn_action_land_style"
        android:layout_below="@id/btn_mults"
        android:text="@string/min" />

    <Button
        android:id="@+id/btn_plus"
        style="@style/btn_action_land_style"
        android:layout_below="@id/btn_minus"
        android:text="@string/plus" />

    <Button
        android:id="@+id/btn_del"
        style="@style/btn_action_land_style"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@id/btn_dev"
        android:layout_toRightOf="@id/btn_dev"
        android:text="@string/del" />

    <Button
        android:id="@+id/btn_equals"
        style="@style/btn_action_land_style"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:text="@string/equals" />

    <View
        style="@style/view_action_panel_style"
        android:layout_below="@id/btn_del"
        android:layout_toRightOf="@id/btn_mults" />

</android.support.percent.PercentRelativeLayout>

    